<Window x:Class="GestionOceanBijoux.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestionOceanBijoux"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="700" Width="1200">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="4*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <!--bouton produit-->
            <Button x:Name="ProduitBouton" Content="Produit" Click="Produit_Click"/>
            <!--bouton categorie-->
            <Button x:Name="CategorieBouton" Content="Categorie" Click="Categorie_Click"/>
        </StackPanel>
        <Frame x:Name="MainFrame" Grid.Column="1" NavigationUIVisibility="Hidden" Navigated="MainFrame_Navigated"></Frame>
    </Grid>
</Window>
